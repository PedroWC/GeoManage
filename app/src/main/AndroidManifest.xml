<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GeoManage"
        tools:targetApi="31">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="YOUR_API_KEY_HERE" />
        <activity
            android:name=".activities.usuario.DetalhesUsuarioActivity"
            android:exported="false" />
        <activity
            android:name=".activities.endereco.DetalhesEnderecoActivity"
            android:exported="false" />
        <activity
            android:name=".activities.cidade.DetalhesCidadeActivity"
            android:exported="false" />
        <activity
            android:name=".activities.endereco.AdicionarEnderecoActivity"
            android:exported="false" />
        <activity
            android:name=".activities.cidade.AdicionarCidadeActivity"
            android:exported="false" />
        <activity
            android:name=".activities.usuario.ListarUsuarioActivity"
            android:exported="false" />
        <activity
            android:name=".activities.endereco.ListarEnderecoActivity"
            android:exported="false" />
        <activity
            android:name=".activities.cidade.ListarCidadeActivity"
            android:exported="false" />
        <activity
            android:name=".activities.OpcoesActivity"
            android:exported="false" />
        <activity
            android:name=".activities.usuario.CriarUsuarioActivity"
            android:exported="false"
            android:theme="@style/Theme.GeoManage" />
        <activity
            android:name=".activities.usuario.LoginUsuarioActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>