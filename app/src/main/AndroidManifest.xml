<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GeoManage"
        tools:targetApi="31">
        <activity
            android:name=".activities.AddressDetailActivity"
            android:exported="false" />
        <activity
            android:name=".activities.CityDetailActivity"
            android:exported="false" />
        <activity
            android:name=".activities.EnderecoListActivity"
            android:exported="false" />
        <activity
            android:name=".activities.CidadeListActivity"
            android:exported="false" />
        <activity
            android:name=".activities.cidadeActivity"
            android:exported="false" />
        <activity
            android:name=".activities.OptionsActivity"
            android:exported="false" />
        <activity
            android:name=".activities.RegisterPageActivity"
            android:exported="false"
            android:theme="@style/Theme.GeoManage" />
        <activity
            android:name=".activities.LoginPageActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>